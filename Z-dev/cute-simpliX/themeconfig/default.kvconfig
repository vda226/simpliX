[%General]
author=Tsu Jan
comment=A theme inspired by Enlightenment
x11drag=true
alt_mnemonic=true
left_tabs=false
joined_tabs=false
attach_active_tab=false
splitter_width=7
scroll_width=12

[PanelButtonCommand]
frame=true
frame.element=button
frame.top=3
frame.bottom=3
frame.left=3
frame.right=3
interior=true
interior.element=button
indicator.size=7
text.shadow=0
text.margin=1
text.iconspacing=3
indicator.element=arrow
text.margin.top=2
text.margin.bottom=2
text.margin.left=3
text.margin.right=3
text.shadow.xshift=2
text.shadow.yshift=2
text.shadow.red=0
text.shadow.green=0
text.shadow.blue=0
text.shadow.alpha=255
text.shadow.depth=1

[PanelButtonTool]
inherits=PanelButtonCommand

[Dock]
inherits=PanelButtonCommand
frame=false
interior=false
text.margin=0

[DockTitle]
inherits=PanelButtonCommand
frame=false
interior=true
interior.element=dock

[IndicatorSpinBox]
inherits=PanelButtonCommand
text.margin=0
indicator.element=arrow
